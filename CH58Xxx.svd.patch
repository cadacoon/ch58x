_svd: CH58Xxx.svd

"*":
  _strip:
    - R8_
    - R16_
    - R32_
  "*":
    _strip:
      - MASK_
      - RB_
      - R8_
      - R16_
      - R32_

SYS:
  CK32K_CONFIG:
    _modify:
      32K_CLK_PIN:
        name: CLK_32K_PIN
  _add:
    FLASH_CFG:
      description: Flash configuration
      addressOffset: 0x807
      size: 8
      access: read-write

ADC:
  _strip:
    - ADC_
  "*":
    _strip:
      - ADC_

GPIOA:
  _strip:
    - PA_
  "*":
    _strip:
      - PA_

GPIOB:
  _strip:
    - PB_
  "*":
    _strip:
      - PB_

TMR0:
  _strip:
    - TMR0_
  "*":
    _strip:
      - TMR_
      - TMR0_

TMR1:
  _strip:
    - TMR1_
  "*":
    _strip:
      - TMR_
      - TMR1_

TMR2:
  _strip:
    - TMR2_
  "*":
    _strip:
      - TMR_
      - TMR2_

TMR3:
  _strip:
    - TMR3_
  "*":
    _strip:
      - TMR_
      - TMR3_

UART0:
  _strip:
    - UART0_
  "*":
    _strip:
      - UART0_

UART1:
  _strip:
    - UART1_
  "*":
    _strip:
      - UART1_

UART2:
  _strip:
    - UART2_
  "*":
    _strip:
      - UART2_

UART3:
  _strip:
    - UART3_
  "*":
    _strip:
      - UART3_

SPI0:
  _strip:
    - SPI0_
  "*":
    _strip:
      - SPI_
      - SPI0_
  CTRL_MOD:
    _modify:
      "2WIRE_MOD":
        name: "TWO_WIRE_MOD"

SPI1:
  _strip:
    - SPI1_
  "*":
    _strip:
      - SPI_
      - SPI1_
  CTRL_MOD:
    _modify:
      "2WIRE_MOD":
        name: "TWO_WIRE_MOD"

I2C:
  _strip:
    - I2C_
  "*":
    _strip:
      - I2C_

USB:
  _strip:
    - USB_
  "*":
    _strip:
      - USB_

USB2:
  _strip:
    - USB2_
  "*":
    _strip:
      - USB2_

PFIC:
  _strip:
    - PFIC_
  "*":
    _strip:
      - PFIC_

Systick:
  _strip:
    - STK_
  "*":
    _strip:
      - STK_
